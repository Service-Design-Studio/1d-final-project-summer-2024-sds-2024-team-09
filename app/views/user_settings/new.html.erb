<h1>Set Up Your Settings</h1>
<%= form_with(model: @user_setting, local: true) do |form| %>
  <div class="form-group">
    <%= form.label :app_id %>
    <%= form.text_field :app_id, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= form.label :token %>
    <%= form.text_field :token, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= form.label :channel %>
    <%= form.text_field :channel, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= form.submit 'Save Settings', class: 'btn btn-primary' %>
  </div>
<% end %>
